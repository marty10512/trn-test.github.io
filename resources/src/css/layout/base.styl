html, body {
  height: 100%;
  font-size: 16px;
  width: 1440px;
  font-family: 'Source Sans Pro', sans-serif;
}

body{
  margin: 0;
  -webkit-font-smoothing: antialiased;
  -moz-animation-smoothing: grayscale;
  line-height: 1.42;
  touch-action: none;
}

ul {
  margin-block-start: 0px;
  margin-block-end: 0px;
  margin: 0;
  padding: 0;
  list-style: none;
}

li {
  list-style: none;
}

button {
  border: none;
  background: transparent;
  cursor: pointer;
}

h1, h2, h3, h4, h5, h6 {
  margin: 0;
  font-weight: normal;
}

td {
  border-collapse: collapse;
  border-spacing: 0;
}

* {
  box-sizing: border-box;
}

a {
  display: inline;
  text-decoration: none;
  color: inherit;
}

p {
  margin-block-start: 0px;
  margin-block-end: 0px;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
  padding-inline-start: 0px;
}

input, textarea, select {
  -webkit-appearance: none;
}

.wrapper
  width 1180px
  height 500px

.maincontent
  position relative

.visuallyHidden 
  position: absolute;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px;
  width: 1px;
  margin: -1px;
  padding: 0;
  border: 0;